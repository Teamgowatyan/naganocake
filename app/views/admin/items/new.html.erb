          <%= form_with model: @item do |f| %>
            <h5>商品画像</h5>
              <%= f.file_field :image, accept: "image/*" %>
            <h5>商品名</h5>
              <%= f.text_field :name %>
            <h5>商品説明</h5>
              <%= f.text_area :introduction %>
            <h5>ジャンル</h5>
              <!--<%= f.collection_select :genre_id, Genre.all, :id, :name %>-->
            <h5>税抜価格</h5>
              <!--<%= f.text_field :unit_price_without_tax, autofocus: true %>-->
            <h5>販売ステータス</h5>
              <!--<%= f.select :sale_status, ["販売中", "販売停止"] %>-->
            <%= f.submit '新規登録' %>
            
          <% end %>